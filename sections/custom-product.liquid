      <ul class="builder_tabs">
          <li class="tab-link current" data-tab="builder_tabstabcontent-1">
            {% comment %} {{ card_product.featured_media | image_url: width: 165 }} {% endcomment %}
            <img src="https://treatsstudio.co.uk/cdn/shop/files/switches_ballons.png?v=1709652423" loading="lazy">
            <span>PAVED BALLOONS</span>
          </li>
          <li class="tab-link" data-tab="builder_tabstabcontent-2">
            <img src="https://treatsstudio.co.uk/cdn/shop/files/switches_HEART.png?v=1709652423" loading="lazy">
            <span>HEARTS</span>
          </li>
          <li class="tab-link" data-tab="builder_tabstabcontent-3">
            <img src="https://treatsstudio.co.uk/cdn/shop/files/switches_letters.png?v=1709652423" loading="lazy">
            <span>GOLD BALLOONS</span>
          </li>
      </ul>
      <p class="current_builder_active_tabs">PAVED BALLOONS</p>
        {% for letter_product in letters_collection.products %}
          {% if letter_product.title == "Paved Balloon" %}
            <ul class="builder_tabstabcontent current" id="builder_tabstabcontent-1">
              {% for variant in letter_product.variants %}
                <li class="alphabetical_icon" dataproducttitle="balloon" datatitle="{{ variant.title }}" dataid="{{ variant.id }}" dataprice="{{ variant.price | money }}">
                  <img src="{{ variant.featured_image | img_url: 'medium' }}" alt="{{ variant.title }}" type_of_necklace="letter" loading="lazy">
                  <span>ADD</span>
                </li>
              {% endfor %}
            </ul>
          {%- elsif letter_product.title == "Paved Heart Initial Pendant Gold" -%}
            <ul class="builder_tabstabcontent " id="builder_tabstabcontent-2">
              {% for variant in letter_product.variants %}
                <li class="alphabetical_icon " dataproducttitle="heart" datatitle="{{ variant.title }}" dataid="{{ variant.id }}" dataprice="{{ variant.price | money }}">
                  <img src="{{ variant.featured_image | img_url: 'medium' }}" alt="{{ variant.title }}" type_of_necklace="heart" loading="lazy">
                  <span>ADD</span>
                </li>
              {% endfor %}
            </ul>
          {%- else -%}
            <ul class="builder_tabstabcontent " id="builder_tabstabcontent-3">
              {% for variant in letter_product.variants %}
                <li class="alphabetical_icon " dataproducttitle="silver" datatitle="{{ variant.title }}" dataid="{{ variant.id }}" dataprice="{{ variant.price | money }}">
                  <img src="{{ variant.featured_image | img_url: 'medium' }}" alt="{{ variant.title }}" type_of_necklace="smooth-gold" loading="lazy">
                  <span>ADD</span>
                </li>
              {% endfor %}
            </ul>
          {% endif %}
        {% endfor %}
